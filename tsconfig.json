{
   "angularCompilerOptions": {
      "genDir": "./dist/aot"
   },
   "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
         "es2015",
         "dom"
      ],
      "mapRoot": "",
      "module": "es2015",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "outDir": "dist",
      "rootDir": ".",
      "skipLibCheck": true,
      "sourceMap": true,
      "target": "es5"
   }
}
